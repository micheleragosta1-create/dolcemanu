{
  "name": "cioccolatini-michele",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "next lint",
    "test:orders": "node test-orders-e2e.js",
    "create:admin": "node scripts/create-super-admin.js",
    "seed:products": "node scripts/seed-products.js",
    "db:dump:schema:prod": "supabase db dump --schema public --file supabase-schema.sql --project-ref $SUPABASE_PROD_REF",
    "db:dump:data:prod": "supabase db dump --data-only --schema public --file supabase-data-$(date +%Y%m%d).sql --project-ref $SUPABASE_PROD_REF",
    "db:apply:schema:test": "supabase db execute --file supabase-schema.sql --project-ref $SUPABASE_TEST_REF",
    "db:apply:data:test": "supabase db execute --file supabase-data-$(date +%Y%m%d).sql --project-ref $SUPABASE_TEST_REF",
    "db:push:test": "supabase db push --project-ref $SUPABASE_TEST_REF",
    "db:push:prod": "supabase db push --project-ref $SUPABASE_PROD_REF"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.2.1",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.54.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
